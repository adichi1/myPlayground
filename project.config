[project]
	description = my Playground
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/adichi1
[access "refs/for/refs/*"]
	read = group user/adichi1
	push = group user/adichi1
	label-Code-Review = -2..+2 group user/adichi1
	label-Verified = -1..+1 group user/adichi1
	rebase = group user/adichi1
	removeReviewer = group user/adichi1
	submit = group user/adichi1
[access "refs/heads/*"]
	read = group user/adichi1
	create = group user/adichi1
	push = +force group user/adichi1
	pushMerge = group user/adichi1
[access "refs/tags/*"]
	push = +force group user/adichi1
	pushTag = group user/adichi1
	pushSignedTag = group user/adichi1
